{
    "PreScripts": {
        "ScriptBaseDirectory": "../CustomSSDTMigrationScripts.Integration.Tests/Scripts/PreScripts",
        "ScriptNamePattern": "^(\\d{14})_(.*).sql",
        "ScriptRecursiveSearch": true,
        "ExecutionFilterMode": "all",
        "ExecutionFilterValue": "",

        "TreatScriptNamePatternMismatchAsError": true,
        "TreatHashMismatchAsError": true
    },

    "PostScripts": {
        "ScriptBaseDirectory": "../CustomSSDTMigrationScripts.Integration.Tests/Scripts/PostScripts",
        "ScriptNamePattern": "^(\\d{14})_(.*).sql",
        "ScriptRecursiveSearch": true,
        "ExecutionFilterMode": "all",
        "ExecutionFilterValue": "",

        "TreatScriptNamePatternMismatchAsError": true,
        "TreatHashMismatchAsError": true
    },

    "ReferenceDataScripts": {
        "ScriptBaseDirectory": "../CustomSSDTMigrationScripts.Integration.Tests/Scripts/ReferenceDataScripts",
        "ScriptNamePattern": "^(\\d+)_(.*).sql",
        "ScriptRecursiveSearch": true,
        "ExecutionFilterMode": "all",
        "ExecutionFilterValue": "",

        "TreatScriptNamePatternMismatchAsError": true,
        "TreatHashMismatchAsError": true
    }
}